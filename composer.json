{
    "name": "forge/habbo-api",
    "description": "Headless API for a Habbo retro CMS using Slim Framework",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "slim/slim": "^4.12",
        "slim/psr7": "^1.3",
        "monolog/monolog": "^2.0",
        "vlucas/phpdotenv": "^5.5",
        "php-di/php-di": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
